{
    "name": "hamdallah90/elasticsearch-eloquent",
    "type": "library",
    "description": "Elasticsearch functionality like Laravel Eloquent models.",
    "keywords": ["elasticsearch", "eloquent"],
    "license": "MIT",
    "require": {
        "php": "^8.2",
        "elasticsearch/elasticsearch": "^8.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0 || ^9.5.21",
        "squizlabs/php_codesniffer": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Isswp101\\Persimmon\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Isswp101\\Persimmon\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "check-style": "vendor/bin/phpcs src tests",
        "fix-style": "vendor/bin/phpcbf src tests"
    },
    "config": {
        "sort-packages": true
    }
}
